  "version": "0.2.0",
  "configurations": [
    {
      "name": "vue2 via NPM Node",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "serve"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "msedge vue2 via NPM Browser",
      "url": " http://10.10.10.134:8080"
    },
    {
      "type": "pwa-chrome",
      "name": "chrome vue2 via NPM Browser",
      "request": "launch",
      "url": "http://10.10.10.134:8080/"
    }
  ]
